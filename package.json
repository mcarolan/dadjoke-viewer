{
  "name": "dadjoke-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract-api-version": "npx ts-node -e \"console.log(require('./src/config/config').API_VERSION);\" > .api-version",
    "test:pact": "jest --testPathPattern=tests/pact",
    "publish:pacts": "PACT_PROVIDER_VERSION=$(cat .api-version) pact-broker publish ./pact/pacts --consumer-app-version=$(git rev-parse --short HEAD) --tag=$PACT_PROVIDER_VERSION --broker-base-url https://lasting-neatly-pig.ngrok-free.app"
  },
  "dependencies": {
    "next": "15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@pact-foundation/pact": "^14.0.0",
    "@pact-foundation/pact-cli": "^16.0.7",
    "@pact-foundation/pact-node": "^10.18.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axios": "^1.8.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5"
  }
}
